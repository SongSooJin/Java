package app;

import lib.Calculator;

public class GoodCalc extends Calculator {
	public  int add(int a, int b) { return a+b;}
	public  int subtract(int a, int b) { return a-b;}
	public  float average(int[] a) {
		float s = 0.0f;
		for(int i = 0; i<a.length; i++) {
			s += a[i];
		}
		return s/a.length;
	} 

}
