abstract class DObject {
	public abstract void draw();
}

class Line extends DObject {
	public void draw() {
		 System.out.println("Line");
	}
}

class Rect extends DObject {
	public void draw() {
		 System.out.println("Rect");
	}
}

class Circle extends DObject {
	public void draw() {
		 System.out.println("Circle");
	}
}

