package w9;

public class EX14_스트링리터럴과_스트링생성자 {

	public static void main(String[] args) {

		String a = "Hello"; 
		String b = "Java"; 
		String c = "Hello";
		if(a==c) System.out.println("a==c");
		
		String d = new String("Hello");
		String e = new String("Java");
		String f = new String("Hello");
	}

}
