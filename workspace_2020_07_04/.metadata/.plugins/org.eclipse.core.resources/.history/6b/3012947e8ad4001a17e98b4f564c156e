package w10;

import java.util.Vector;

public class Ex16_Vector {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Vector<Integer> v = new Vector<Integer>();
		System.out.println(v.capacity()); // 용량 10
		v.add(7);
		v.add(new Integer(4));
		v.add(-1);
		
		int s = v.size(); //요소의 개수
		int c = v.capacity(); // 용량의 개수
		System.out.println(v.size()); // 요소의 개수3
		System.out.println(v.capacity()); // 저장할수 있는 용량의 크기 10
	}

}
